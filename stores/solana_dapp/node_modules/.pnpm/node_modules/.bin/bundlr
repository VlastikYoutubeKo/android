#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/brien/bringyour/android/stores/solana_dapp/node_modules/.pnpm/@bundlr-network+client@0.8.9_debug@4.3.4/node_modules/@bundlr-network/client/build/node/node_modules:/Users/brien/bringyour/android/stores/solana_dapp/node_modules/.pnpm/@bundlr-network+client@0.8.9_debug@4.3.4/node_modules/@bundlr-network/client/build/node_modules:/Users/brien/bringyour/android/stores/solana_dapp/node_modules/.pnpm/@bundlr-network+client@0.8.9_debug@4.3.4/node_modules/@bundlr-network/client/node_modules:/Users/brien/bringyour/android/stores/solana_dapp/node_modules/.pnpm/@bundlr-network+client@0.8.9_debug@4.3.4/node_modules/@bundlr-network/node_modules:/Users/brien/bringyour/android/stores/solana_dapp/node_modules/.pnpm/@bundlr-network+client@0.8.9_debug@4.3.4/node_modules:/Users/brien/bringyour/android/stores/solana_dapp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/brien/bringyour/android/stores/solana_dapp/node_modules/.pnpm/@bundlr-network+client@0.8.9_debug@4.3.4/node_modules/@bundlr-network/client/build/node/node_modules:/Users/brien/bringyour/android/stores/solana_dapp/node_modules/.pnpm/@bundlr-network+client@0.8.9_debug@4.3.4/node_modules/@bundlr-network/client/build/node_modules:/Users/brien/bringyour/android/stores/solana_dapp/node_modules/.pnpm/@bundlr-network+client@0.8.9_debug@4.3.4/node_modules/@bundlr-network/client/node_modules:/Users/brien/bringyour/android/stores/solana_dapp/node_modules/.pnpm/@bundlr-network+client@0.8.9_debug@4.3.4/node_modules/@bundlr-network/node_modules:/Users/brien/bringyour/android/stores/solana_dapp/node_modules/.pnpm/@bundlr-network+client@0.8.9_debug@4.3.4/node_modules:/Users/brien/bringyour/android/stores/solana_dapp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@bundlr-network/client/build/node/cli.js" "$@"
else
  exec node  "$basedir/../@bundlr-network/client/build/node/cli.js" "$@"
fi
